<span id="todo-count"><strong>{{remaining}}</strong> item(s) left</span>
<ul id="filters" class="radio">
  <li>
    <a href="#" class="radio-item selected" data-filter="all">All</a>
  </li>
  <li>
    <a href="#" class="radio-item" data-filter="active">Active</a>
  </li>
  <li>
    <a href="#" class="radio-item" data-filter="completed">Completed</a>
  </li>
</ul>
{{#if completed}}
<button id="clear-completed">Clear completed ({{completed}})</button>
{{/if}}